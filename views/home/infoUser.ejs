<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <!-- <link rel="stylesheet" href="/css/style2.css"> -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans:wght@300;400;500;600&display=swap" rel="stylesheet">

    <link rel="shortcut icon" href="favicon.ico">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,700,800">

    <link rel="stylesheet" href="/css/style4.css">
    <link href="/css/conditional-preet.css" type="text/css" rel="stylesheet">

    <title>Gamedify Admin</title>
</head>
<body>



    <div class="infoUser">
        <div class="inside-infoUser">
            <div class="Aa">
                <div class="ia ">
                    <div class="display1">
                        <% if(user.correctQues.length<=0) { %>
                            <div class="cccccc">No Question done till yet or not correct till yet</div>
                            <%  } %>
                        <div class="row">
                            <% for(let cq of user.correctQues){ %>
                            <div class="  p-2">
                                <div class="dsh_stats_w">
                                <div class="stats_head">
                                <h2>Correct Question</h2>
                                
                                <div class="stat_sub_hed">
                                <span>Total Questions: <b><%=user.correctQues.length%></b></span>
                                </div>
                                
                                </div>
                                 
                                 
                                <div class="dsh_content">
                                <div class="dsh_table_ot">
                                <table width="100%" border="0" class="table">
                                <thead>
                                <tr>
                                <!-- <th width="60%">Question</th> -->
                                </tr>
                                </thead>
                                
                                <tbody>
                               
                                <tr>
                                <td><%=cq.question%></td>
                                
                                </tr>
                                
                                </tbody>
                                </table>
                                </div> 
                                 </div>
                                  </div>
                                </div>
                            <% } %>    
                      </div>
                    </div>
                    <div class="display2">
                        <% if(user.incorrectQues.length<=0) { %>
                            <div class="cccccc">No incorrect question</div>
                            <%  } %>
                        <div class="row">
                            <% for(let iq of user.incorrectQues){ %>
                            <div class="  p-2">
                                <div class="dsh_stats_w">
                                <div class="stats_head">
                                <h2>Incorrect Question</h2>
                                
                                <div class="stat_sub_hed">
                                <span>Total Questions: <b><%=user.incorrectQues.length%></b></span>
                                </div>
                                
                                </div>
                                 
                                 
                                <div class="dsh_content">
                                <div class="dsh_table_ot">
                                <table width="100%" border="0" class="table">
                                <thead>
                                <tr>
                                <!-- <th width="60%">Question</th> -->
                                </tr>
                                </thead>
                                
                                <tbody>
                               
                                <tr>
                                <td><%=iq.question%></td>
                                
                                </tr>
                                
                                </tbody>
                                </table>
                                </div> 
                                 </div>
                                  </div>
                                </div>
                            <% } %>    
                      </div>
                    </div>
                    <div class="qqqq display3" id="aaaa">
                        <div>
                            <div id="img">img</div>
                           <h4> Username : </h4><div id="username">@<%=user.username%></div>
                        </div>
                        <!-- <div></div> -->
                    </div>
                    <div class="qqqq1 display3" id="bbbb"></div>
                    <!-- <div class="qqqq display3" id="cccc"></div> -->
                </div>
                <div class="ib">

                    <div class="SSS">
                    <div class="acc_box dashb_bx">
                    <div class="dash_rel_ico">
                    <span class="fas fa-regular fa-book" style="color:#3fc0e6"></span>
                    </div>
                    
                    <div class="dash_rel_txt">
                    <b><%=user.correctQues.length%></b>
                    <span>Total Correct Question</span>
                    
                    </div>
                    
                    </div></div>

                    <div class="SSS">
                        <div class="acc_box dashb_bx">
                        <div class="dash_rel_ico">
                        <span class="fas fa-regular fa-book" style="color:#3fc0e6"></span>
                        </div>
                        
                        <div class="dash_rel_txt">
                        <b><%=user.incorrectQues.length%></b>
                        <span>Total Incorrect Question</span>
                        
                        </div>
                        
                        </div></div>

                        <div class=" SSS">
                            <div class="acc_box dashb_bx">
                            <div class="dash_rel_ico">
                            <span class="fas fa-regular fa-book" style="color:#3fc0e6"></span>
                            </div>
                            
                            <div class="dash_rel_txt">
                            <b><%=user.points%></b>
                            <span>Total Points</span>
                            
                            </div>
                            
                            </div></div>

                </div>
            </div>
            <div class="Bb">
                <div class="ibb">
                    <a href="/Gamedify/explore"><div class=" bbbb" >DashBoard</div></a>
                    <div class="dashboard  bbbb" onclick="display3()">User</div>
                    <div class="correct bbbb" onclick="display1()">Correct Question</div>
                    <div class="incorrect bbbb" onclick="display2()">Incorrect Question</div>
                    <a href="/Gamedify/logout"><div class=" bbbb" >Logout</div></a>
                </div>
            </div>
        </div>
    </div>






        <script src="/js/startScript.js"></script>
      <script src="/js/script.js"></script>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  </body>
  </html>