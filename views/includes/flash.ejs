<%  if((success && success.length)){  %>
<div class="alert alert-success alert-dismissible fade show  flash2" role="alert" >
    <%=success%>
    <button type="button" class="btn-close flash-btn"  data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>

<%  if((error && error.length)){  %>
    <div class="alert alert-danger alert-dismissible fade show flash2" role="alert">
        <%=error%>
        <button type="button" class="btn-close btn_close flash-btn"   data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>