<% layout('/layouts/boilerplate2.ejs') %>

<div class="content1">
    <div class="inside1-content1"><%=ques[0].chapter.chapterName%></div>
    <div class="inside2-content1"><%=ques[0].chapter.course.courseName%></div>
</div>

<div class="content2">
    <div class="content2-inside">
        <div class="content2-a">
            <div>
                <% if(ques[0].chapter.course.countQuestion>0) { %>
                <%=ques[0].chapter.course.countQuestion%>
                <%} %>
            </div>
            <div>Questions</div>
        </div>
        <div class="content2-b">
            <div>
                <%=users%>
            </div>
            <div>Users</div>
        </div>
    </div>
</div>

<div class="content3">
    <% for(let q of ques){ %>
    <div class="inside-content3">
        <div class="inside-content3-1"><%=q.question%></div>
        <div class="inside-content3-ques" id="iii"><a href="/Gamedify/course/<%=q._id%>/solve">Solve it</a></div>
        <p class="contentP">Solve questions for getting Points</p>
    </div>
    <% } %>
</div>

